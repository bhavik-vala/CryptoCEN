# Copy this file to .env and fill values
AI_PROVIDER=claude
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=

# LinkedIn
LINKEDIN_ACCESS_TOKEN=
# Use your personal URN for posting (e.g. urn:li:person:vvQE2g2rkz)
LINKEDIN_PERSON_ID=

# Set to 'false' for live posting
TEST_MODE=true

# Content profile: 'valtrilabs' or 'arab_global_crypto'
CONTENT_PROFILE=valtrilabs

# Schedule (IST 11:00 default)
POST_TIME_HOUR=11
POST_TIME_MINUTE=0
TIMEZONE=Asia/Kolkata
